    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55cad373f700_0, 0, 1;
    %pushi/vec4 258, 0, 32;
T_3.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
