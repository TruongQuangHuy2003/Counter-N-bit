:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x55cad3710080 .scope module, "test_bench" "test_bench" 2 2;
 .timescale -9 -12;
P_0x55cad3710210 .param/l "N" 0 2 3, +C4<00000000000000000000000000001000>;
v0x55cad373f4a0_0 .var "clk", 0 0;
v0x55cad373f560_0 .net "count", 7 0, v0x55cad3707890_0;  1 drivers
v0x55cad373f600_0 .var "count_clr", 0 0;
v0x55cad373f700_0 .var "count_dir", 0 0;
v0x55cad373f7d0_0 .var "count_en", 0 0;
v0x55cad373f8c0_0 .var "exp_count", 7 0;
v0x55cad373f960_0 .var "exp_overflow", 0 0;
v0x55cad373fa00_0 .net "overflow", 0 0, v0x55cad373ed20_0;  1 drivers
v0x55cad373fad0_0 .var "rst_n", 0 0;
E_0x55cad36d4f10 .event posedge, v0x55cad3706000_0;
S_0x55cad36d4490 .scope module, "dut" "counter" 2 12, 3 1 0, S_0x55cad3710080;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst_n";
    .port_info 2 /INPUT 1 "count_en";
    .port_info 3 /INPUT 1 "count_clr";
    .port_info 4 /INPUT 1 "count_dir";
    .port_info 5 /OUTPUT 1 "overflow";
    .port_info 6 /OUTPUT 8 "count";
P_0x55cad36d4670 .param/l "N" 0 3 1, +C4<00000000000000000000000000001000>;
v0x55cad3706000_0 .net "clk", 0 0, v0x55cad373f4a0_0;  1 drivers
v0x55cad3707890_0 .var "count", 7 0;
v0x55cad370ab20_0 .net "count_clr", 0 0, v0x55cad373f600_0;  1 drivers
v0x55cad3707da0_0 .net "count_dir", 0 0, v0x55cad373f700_0;  1 drivers
v0x55cad371ddd0_0 .net "count_en", 0 0, v0x55cad373f7d0_0;  1 drivers
v0x55cad373ed20_0 .var "overflow", 0 0;
v0x55cad373ede0_0 .net "rst_n", 0 0, v0x55cad373fad0_0;  1 drivers
E_0x55cad36d4cc0/0 .event negedge, v0x55cad373ede0_0;
E_0x55cad36d4cc0/1 .event posedge, v0x55cad3706000_0;
E_0x55cad36d4cc0 .event/or E_0x55cad36d4cc0/0, E_0x55cad36d4cc0/1;
S_0x55cad373ef80 .scope task, "verify" "verify" 2 27, 2 27 0, S_0x55cad3710080;
 .timescale -9 -12;
v0x55cad373f180_0 .var "count", 7 0;
v0x55cad373f260_0 .var "exp_count", 7 0;
v0x55cad373f340_0 .var "exp_overflow", 0 0;
v0x55cad373f3e0_0 .var "overflow", 0 0;
TD_test_bench.verify ;
    %vpi_call 2 31 "$display", "At time: %t, rst_n = %b, count_en = %b, count_clr = %b, count_dir = %b", $time, v0x55cad373fad0_0, v0x55cad373f7d0_0, v0x55cad373f600_0, v0x55cad373f700_0 {0 0 0};
    %load/vec4 v0x55cad373f3e0_0;
    %load/vec4 v0x55cad373f340_0;
    %cmp/e;
    %flag_get/vec4 4;
    %jmp/0 T_0.2, 4;
    %load/vec4 v0x55cad373f180_0;
    %load/vec4 v0x55cad373f260_0;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
T_0.2;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %vpi_call 2 33 "$display", "---------------------------------------------------------------------------------------------------------------------------------" {0 0 0};
    %vpi_call 2 34 "$display", "PASSED: Expected count = %d,Got count = %d, Expected overflow = 1'b%b, Got overflow: 1'b%b", v0x55cad373f260_0, v0x55cad373f180_0, v0x55cad373f340_0, v0x55cad373f3e0_0 {0 0 0};
    %vpi_call 2 35 "$display", "---------------------------------------------------------------------------------------------------------------------------------" {0 0 0};
    %jmp T_0.1;
T_0.0 ;
    %vpi_call 2 37 "$display", "---------------------------------------------------------------------------------------------------------------------------------" {0 0 0};
    %vpi_call 2 38 "$display", "FAILED: Expected count = %d,Got count = %d, Expected overflow = 1'b%b, Got overflow: 1'b%b", v0x55cad373f260_0, v0x55cad373f180_0, v0x55cad373f340_0, v0x55cad373f3e0_0 {0 0 0};
    %vpi_call 2 39 "$display", "---------------------------------------------------------------------------------------------------------------------------------" {0 0 0};
T_0.1 ;
    %end;
    .scope S_0x55cad36d4490;
T_1 ;
    %wait E_0x55cad36d4cc0;
    %load/vec4 v0x55cad373ede0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x55cad3707890_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55cad373ed20_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x55cad370ab20_0;
    %flag_set/vec4 8;
    %jmp/0 T_1.2, 8;
    %pushi/vec4 0, 0, 8;
    %jmp/1 T_1.3, 8;
T_1.2 ; End of true expr.
    %load/vec4 v0x55cad371ddd0_0;
    %nor/r;
    %flag_set/vec4 9;
    %jmp/0 T_1.4, 9;
    %load/vec4 v0x55cad3707890_0;
    %jmp/1 T_1.5, 9;
T_1.4 ; End of true expr.
    %load/vec4 v0x55cad3707da0_0;
    %flag_set/vec4 10;
    %jmp/0 T_1.6, 10;
    %load/vec4 v0x55cad3707890_0;
    %addi 1, 0, 8;
    %jmp/1 T_1.7, 10;
T_1.6 ; End of true expr.
    %load/vec4 v0x55cad3707890_0;
    %subi 1, 0, 8;
    %jmp/0 T_1.7, 10;
 ; End of false expr.
    %blend;
T_1.7;
    %jmp/0 T_1.5, 9;
 ; End of false expr.
    %blend;
T_1.5;
    %jmp/0 T_1.3, 8;
 ; End of false expr.
    %blend;
T_1.3;
    %assign/vec4 v0x55cad3707890_0, 0;
    %load/vec4 v0x55cad3707da0_0;
    %flag_set/vec4 8;
    %jmp/0 T_1.8, 8;
    %load/vec4 v0x55cad3707890_0;
    %cmpi/e 255, 0, 8;
    %flag_get/vec4 4;
    %jmp/0 T_1.10, 4;
    %load/vec4 v0x55cad371ddd0_0;
    %and;
T_1.10;
    %jmp/1 T_1.9, 8;
T_1.8 ; End of true expr.
    %load/vec4 v0x55cad3707890_0;
    %cmpi/e 0, 0, 8;
    %flag_get/vec4 4;
    %jmp/0 T_1.11, 4;
    %load/vec4 v0x55cad371ddd0_0;
    %and;
T_1.11;
    %jmp/0 T_1.9, 8;
 ; End of false expr.
    %blend;
T_1.9;
    %assign/vec4 v0x55cad373ed20_0, 0;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x55cad3710080;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55cad373f4a0_0, 0, 1;
T_2.0 ;
    %delay 5000, 0;
    %load/vec4 v0x55cad373f4a0_0;
    %inv;
    %store/vec4 v0x55cad373f4a0_0, 0, 1;
    %jmp T_2.0;
    %end;
    .thread T_2;
    .scope S_0x55cad3710080;
T_3 ;
    %vpi_call 2 45 "$dumpfile", "test_bench.vcd" {0 0 0};
    %vpi_call 2 46 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55cad3710080 {0 0 0};
    %vpi_call 2 48 "$display", "----------------------------------------------------------------------------------------------------------------------------------" {0 0 0};
    %vpi_call 2 49 "$display", " ---------------------------------------- TESTBENCH FOR COUNTER ------------------------------------------------------------------" {0 0 0};
    %vpi_call 2 50 "$display", "----------------------------------------------------------------------------------------------------------------------------------" {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55cad373fad0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55cad373f7d0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55cad373f600_0, 0, 1;
