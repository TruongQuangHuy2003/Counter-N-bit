    %delay 1000, 0;
    %load/vec4 v0x55cad373f960_0;
    %store/vec4 v0x55cad373f340_0, 0, 1;
    %load/vec4 v0x55cad373fa00_0;
    %store/vec4 v0x55cad373f3e0_0, 0, 1;
    %load/vec4 v0x55cad373f8c0_0;
